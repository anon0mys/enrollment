<h2><%= @student.name %></h2>

<ul>
  <% @addresses.each do |address| %>
    <li>
      <%= address.description %>
      <%= address.street %>
      <%= address.city %>
      <%= address.state %>
      <%= address.zipcode %>
    </li>
  <% end %>
</ul>

<ul>
  <% @courses.each do |course| %>
    <li><%= course.name %></li>
  <% end %>
</ul>
